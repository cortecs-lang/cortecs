        "clang_rt.asan_dynamic_runtime_thunk.lib",
        "clang_rt.fuzzer_interceptors.lib",
        "clang_rt.stats.lib",
    ],
    cmd = "tar.exe xf $(location libclang.tar.gz) -C $(@D)",
    tags = ["no-remote-cache"],
    visibility = ["//visibility:public"],
